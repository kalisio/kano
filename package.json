{
  "name": "aktnmap",
  "description": "Kalisio App Template",
  "version": "0.1.0",
  "homepage": "https://github.com/kalisio/aktnmap",
  "keywords": [
    "kalisio"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/aktnmap.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/aktnmap/issues"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:patch": "cd api && npm run release:patch && cd .. && npm version --force patch && npm run publish && cordova-set-version cordova/config.xml",
    "release:minor": "cd api && npm run release:minor && cd .. && npm version --force minor && npm run publish && cordova-set-version cordova/config.xml",
    "release:major": "cd api && run release:major && cd .. && npm version --force major && npm run publish && cordova-set-version cordova/config.xml",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "prod": "cd api && npm run prod",
    "build": "node build/script.build.js",
    "cordova:remove:android": "cd cordova && cordova platform remove android && cd ..",
    "cordova:remove:ios": "cd cordova && cordova platform remove ios && cd ..",
    "cordova:remove": "npm run cordova:remove:android && npm run cordova:remove:ios",
    "cordova:clean": "cd cordova && cordova clean remove ios && cd ..",
    "cordova:add:android": "cd cordova && cordova platform add android@6.4 && cd ..",
    "cordova:add:ios": "cd cordova && cordova platform add ios && cd ..",
    "cordova:add": "npm run cordova:add:android && npm run cordova:add:ios",
    "cordova:build:debug": "cd cordova && cordova build --debug && cd ..",
    "cordova:build:release": "cd cordova && cordova build --release && cd ..",
    "cordova:build": "npm run cordova:build:debug && npm run cordova:build:release",
    "cordova:fastlane": "cd cordova && cross-env-shell \"fastlane $NODE_APP_INSTANCE\" && cd ..",
    "cordova:supply:android": "npm run cordova:add:android && npm run cordova:build && npm run cordova:fastlane",
    "cordova:dev": "cd cordova && cordova run --debug && cd ..",
    "cordova:prod": "cd cordova && cordova run --release && cd ..",
    "cordova": "npm run cordova:dev",
    "analyze:stats": "webpack --config build/webpack.prod.conf.js --profile --json > build-stats.json",
    "analyze:treemap": "webpack-bundle-analyzer build-stats.json dist",
    "analyze": "npm run analyze:stats && npm run analyze:treemap",
    "lint": "standard src/**/*.js src/**/*.vue test/**/*.js --fix",
    "benchmark": "node ./benchmark",
    "cafe": "testcafe firefox test/*.test.js -S -s screenshots",
    "test:server": "cd api && npm run test",
    "test:client": "cross-env NODE_ENV=production testcafe chrome:headless test/*.test.js -f Basic --app \"npm run prod\" --app-init-delay 10000 -S -s screenshots",
    "test": "npm-run-all --parallel test:**"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "testcafe",
      "html"
    ],
    "sourceType": "module",
    "globals": [
      "DEV",
      "PROD",
      "__THEME",
      "cordova",
      "test",
      "fixture"
    ]
  },
  "dependencies": {
    "@panter/vue-i18next": "^0.9.1",
    "ajv": "^5.2.2",
    "ajv-i18n": "2",
    "annyang": "^2.6.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "casl": "^1.0.2",
    "cesium": "^1.38.0",
    "cross-env": "^5.1.4",
    "fastclick": "^1.0.6",
    "feathers-authentication-management": "^1.0.0",
    "feathers-client": "^2.2.0",
    "feathers-hooks": "^2.0.1",
    "feathers-reactive": "^0.4.1",
    "i18next": "^10.5.0",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "leaflet": "^1.0.3",
    "leaflet-basemaps": "^0.2.0",
    "leaflet-fa-markers": "^0.0.5",
    "leaflet-filelayer": "^1.0.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-measure": "^2.1.7",
    "leaflet.markercluster": "^1.0.5",
    "leaflet.vectorgrid": "^1.3.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.6.0",
    "material-design-icons": "^3.0.1",
    "npm-run-all": "^4.1.2",
    "password-generator": "^2.2.0",
    "quasar-extras": "0.0.8",
    "quasar-framework": "0.14.7",
    "roboto-fontface": "^0.7.0",
    "rxjs": "^5.4.1",
    "socket.io-client": "^2.0.1",
    "testcafe": "^0.19.2",
    "testcafe-vue-selectors": "^3.0.0",
    "togeojson": "^0.16.0",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "vue": "2.5.2",
    "vue-avatar": "^2.0.0",
    "vue-router": "^2.7.0",
    "vue2-dropzone": "^3.0.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "clui": "^0.3.6",
    "colors": "^1.1.2",
    "commander": "^2.15.0",
    "config": "^1.26.1",
    "connect-history-api-fallback": "^1.1.0",
    "cordova": "^8.0.0",
    "cordova-set-version": "^6.0.3",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "glob": "^7.1.2",
