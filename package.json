{
  "name": "kano",
  "description": "Kalisio Kano Application",
  "version": "0.1.0",
  "homepage": "https://github.com/kalisio/kapp",
  "keywords": [
    "kalisio"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/kapp.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/kapp/issues"
  },
  "scripts": {
    "version": "cordova-set-version cordova/config.tpl.xml",
    "release:patch": "cd api && npm run release:patch && cd .. && npm version --no-git-tag-version --force patch",
    "release:minor": "cd api && npm run release:minor && cd .. && npm version --no-git-tag-version --force minor",
    "release:major": "cd api && npm run release:major && cd .. && npm version --no-git-tag-version --force major",
    "changelog": "github_changelog_generator -u kalisio -p kapp && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "clean": "quasar clean",
    "dev": "quasar dev",
    "prod": "cd api && npm run prod",
    "build": "quasar build",
    "cordova:remove:android": "cd cordova && cordova platform remove android && cd ..",
    "cordova:remove:ios": "cd cordova && cordova platform remove ios && cd ..",
    "cordova:remove": "npm run cordova:remove:android && npm run cordova:remove:ios",
    "cordova:clean": "cd cordova && cordova clean && cd ..",
    "cordova:add:android": "cd cordova && envsub -s dollar-both config.xml.tpl config.xml && cordova platform add android@~7.1.1 && cd ..",
    "cordova:add:ios": "cd cordova && envsub -s dollar-both config.xml.tpl config.xml && cordova platform add ios@~5.0.0 && cd ..",
    "cordova:add": "npm run cordova:add:android && npm run cordova:add:ios",
    "cordova:build:debug": "cd cordova && cordova build --debug --device && cd ..",
    "cordova:build:release": "cd cordova && cordova build --release --device && cd ..",
    "cordova:build": "npm run cordova:build:debug && npm run cordova:build:release",
    "cordova:build:android": "npm run cordova:add:android && npm run cordova:build:release",
    "cordova:build:ios": "npm run cordova:add:ios && npm run cordova:build:release",
    "cordova:dev": "cd cordova && cordova run --debug && cd ..",
    "cordova:prod": "cd cordova && cordova run --release && cd ..",
    "cordova": "npm run cordova:dev",
    "analyze:stats": "webpack --config build/webpack.prod.conf.js --profile --json > build-stats.json",
    "analyze:treemap": "webpack-bundle-analyzer build-stats.json dist",
    "analyze": "npm run analyze:stats && npm run analyze:treemap",
    "lint": "standard src/**/*.js src/**/*.vue test/**/*.js --fix",
    "benchmark": "node ./benchmark",
    "cafe": "testcafe firefox test/*.test.js -S -s screenshots",
    "rec": "node ./tutorials/record.js",
    "test:server": "cd api && npm run test",
    "test:client": "cross-env NODE_ENV=production testcafe chrome:headless test/*.test.js -f Basic --app \"npm run prod\" --app-init-delay 10000 -S -s screenshots",
    "test": "npm-run-all --parallel test:**"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "testcafe",
      "html"
    ],
    "sourceType": "module",
    "globals": [
      "DEV",
      "PROD",
      "__THEME",
      "cordova",
      "test",
      "fixture"
    ]
  },
  "dependencies": {
    "@feathersjs/client": "^3.5.4",
    "@panter/vue-i18next": "^0.9.1",
    "@quasar/extras": "^1.2.0",
    "@turf/turf": "^5.1.6",
    "Leaflet.Geodesic": "https://github.com/henrythasler/Leaflet.Geodesic.git",
    "ajv": "^5.2.2",
    "ajv-i18n": "2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "casl": "^1.0.2",
    "cesium": "^1.46.1",
    "chart.js": "^2.7.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "chroma-js": "^1.3.4",
    "cross-env": "^5.1.4",
    "d3": "^4.9.1",
    "fastclick": "^1.0.6",
    "feathers-authentication-management": "^1.0.0",
    "feathers-client": "^2.2.0",
    "feathers-reactive": "^0.7.2",
    "formatcoords": "^1.1.3",
    "georaster": "^0.4.2",
    "i18next": "^10.5.0",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "leaflet": "^1.2.0",
    "leaflet-draw": "^1.0.4",
    "leaflet-fa-markers": "^0.0.5",
    "leaflet-filelayer": "^1.0.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-pixi-overlay": "1.8.1",
    "leaflet-realtime": "https://github.com/kalisio/leaflet-realtime",
    "leaflet-timedimension": "^1.1.0",
    "leaflet-velocity": "^1.2.2",
    "leaflet.markercluster": "^1.0.5",
    "leaflet.vectorgrid": "^1.3.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.6.0",
    "material-design-icons": "^3.0.1",
    "mathjs": "^5.4.0",
    "mime-types-browser": "^0.0.3",
    "moment": "^2.22.2",
    "password-generator": "^2.2.0",
    "pixi.js": "5.0.4",
    "post-robot": "^10.0.10",
    "quasar": "^1.0.5",
    "roboto-fontface": "^0.7.0",
    "showdown": "^1.8.6",
    "socket.io-client": "^2.0.1",
    "togeojson": "^0.16.0",
    "tween.js": "^16.6.0",
    "vue2-dropzone": "^3.6.0",
    "weacast-core": "^1.0.0",
    "weacast-leaflet": "^1.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "commander": "^2.15.0",
    "config": "^1.26.1",
    "connect-history-api-fallback": "^1.1.0",
    "cordova": "^8.0.0",
    "cordova-set-version": "^6.0.3",
    "es6-promise": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "express": "^4.15.4",
    "glob": "^7.1.2",
    "npm-run-all": "^4.1.2",
    "shelljs": "^0.8.1",
    "standard": "^10.0.2",
    "testcafe": "^1.0.1",
    "testcafe-vue-selectors": "^3.0.0",
    "winston": "^2.4.1",
    "worker-farm": "^1.6.0"
  }
}
