    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "vue-loader": "~12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "2.5.2",
    "webpack": "^3.5.2",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "winston": "^2.4.1",
    "worker-farm": "^1.6.0"
  }
}
